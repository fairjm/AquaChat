<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AquaChat.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AquaChat"
    xmlns:pages="using:AquaChat.Pages"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent
        Title="Chat"
        ContentTemplate="{DataTemplate pages:ChatPage}"
        Route="ChatPage" />
        <ShellContent
            Title="Setting"
            ContentTemplate="{DataTemplate pages:SettingPage}"
            Route="SettingPage" />
    </TabBar>
</Shell>
